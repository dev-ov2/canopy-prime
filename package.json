{
  "name": "@canopy/overwolf-electron",
  "productName": "Canopy",
  "version": "2.0.0",
  "description": "Complete tasks, earn points, and plant trees while playing your favorite games!",
  "license": "MIT",
  "author": {
    "name": "OV2",
    "email": "projectluca.dev@gmail.com"
  },
  "main": "./out/main/main.js",
  "scripts": {
    "dev": "cross-env ELECTRON_DISABLE_SANDBOX=1 electron-vite dev -w",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "start": "electron-vite preview",
    "postinstall": "electron-builder install-app-deps",
    "vite:build:app": "electron-vite build",
    "vite:build:app:watch": "electron-vite build --watch",
    "electron:build:win": "electron-builder --win",
    "electron:build:mac": "electron-builder --mac",
    "electron:build:linux": "electron-builder --linux",
    "electron:build:dir": "electron-builder --dir",
    "build:unpack": "run-s vite:build:app electron:build:dir",
    "build:win": "run-s vite:build:app electron:build:win",
    "build:mac": "run-s vite:build:app electron:build:mac",
    "build:linux": "run-s vite:build:app electron:build:linux",
    "ow:dev": "vite dev",
    "ow:build": "vite build",
    "ow:build:win": "run-s vite:build:app ow-electron-builder --c.extraMetadata.name=Canopy --publish=\"never\""
  },
  "overwolf": {
    "packages": []
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dev-ov2/canopy-prime"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@overwolf/ow-electron",
      "better-sqlite3",
      "electron",
      "esbuild"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "3.8.2",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@node-steam/vdf": "^2.2.0",
    "@overwolf/electron-is-overwolf": "0.0.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "better-sqlite3": "^12.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-log": "^5.4.3",
    "electron-store": "10.0.1",
    "electron-updater": "^6.6.2",
    "koffi": "^2.14.1",
    "lucide-react": "^0.541.0",
    "next-themes": "^0.4.6",
    "react-icons": "^5.5.0",
    "swiper": "^11.2.6",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.7",
    "vdf": "^0.0.2",
    "vite-plugin-electron": "^0.29.0",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@eslint/js": "^9.34.0",
    "@overwolf/ow-electron": "37.7.0",
    "@overwolf/ow-electron-builder": "26.0.12",
    "@overwolf/ow-electron-packages-types": "1.0.2",
    "@rushstack/eslint-patch": "^1.12.0",
    "@tailwindcss/vite": "^4.1.12",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@vitejs/plugin-react": "^5.0.1",
    "cross-env": "^10.0.0",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "framer-motion": "^12.23.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "alias": {
    "electron": "@overwolf/ow-electron"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "https://electron-updates.overwolf.com/electron-updates/electron/hbdkmbbfediokpddpgbpeojalkhapldlnfandmim",
        "channel": "latest"
      }
    ]
  }
}
